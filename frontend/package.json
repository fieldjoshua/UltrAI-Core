{
  "name": "ultra-ai",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "description": "Ultra AI Intelligence Multiplication Platform",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "analyze": "ANALYZE=true vite build",
    "build:analyze": "ANALYZE=true vite build",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch --watchAll",
    "test:auto": "jest --config=jest.config.cjs --testPathPattern='src/tests/auto'",
    "test:ui": "jest --config=jest.config.cjs --watch --watchAll --testPathPattern='src/tests/auto'",
    "test:simple": "jest --config jest.simple.config.cjs",
    "prepare": "husky install",
    "analyze-bundle": "NODE_OPTIONS=--max-old-space-size=4096 source-map-explorer 'dist/*.js'",
    "bundlesize": "bundlesize",
    "test:a11y": "jest --config=jest.config.cjs --testPathPattern='src/tests/a11y'",
    "audit": "npm audit --production",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/cache": "^11.14.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-context": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-use-callback-ref": "^1.1.0",
    "@radix-ui/react-use-controllable-state": "^1.1.0",
    "@radix-ui/react-use-layout-effect": "^1.1.0",
    "@radix-ui/react-use-previous": "^1.1.0",
    "@radix-ui/react-use-size": "^1.1.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/react": "^9.11.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4.1.3",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.334.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.22.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@emotion/jest": "^11.13.0",
    "@jest/expect": "^29.7.0",
    "@swc/plugin-emotion": "^9.0.1",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.9.1",
    "babel-jest": "^29.7.0",
    "bundlesize": "^0.18.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^0.24.0",
    "expect": "^29.7.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock": "^29.7.0",
    "jest-util": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.2.5",
    "vite-plugin-compression2": "^1.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,json}": "prettier --write"
  },
  "bundlesize": [
    {
      "path": "./dist/assets/*.js",
      "maxSize": "250 kB"
    },
    {
      "path": "./dist/assets/*.css",
      "maxSize": "50 kB"
    }
  ]
}
