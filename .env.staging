# Staging Environment Variables
# For ultrai-staging-api.onrender.com

# Core Settings
ENVIRONMENT=staging
PORT=10000
DEBUG=true

# Feature Flags
ENABLE_AUTH=true
ENABLE_RATE_LIMIT=true
ENABLE_BILLING=false
ENABLE_PRICING=false

# Authentication
JWT_SECRET=staging-jwt-secret-change-in-production
JWT_REFRESH_SECRET=staging-refresh-secret-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Database (Staging Database)
DATABASE_URL=postgresql://user:password@host:5432/ultrai_staging

# Redis (Staging Redis)
REDIS_URL=redis://localhost:6379/1

# LLM Providers (Test Keys)
OPENAI_API_KEY=your-staging-openai-key
ANTHROPIC_API_KEY=your-staging-anthropic-key
GOOGLE_API_KEY=your-staging-google-key
HUGGINGFACE_API_KEY=your-staging-huggingface-key

# Security
CORS_ALLOWED_ORIGINS=https://ultrai-staging-api.onrender.com,http://localhost:5173,http://localhost:8000

# Monitoring
OTEL_ENABLED=false
SENTRY_DSN=

# Service URLs
FRONTEND_URL=https://ultrai-staging-api.onrender.com
API_URL=https://ultrai-staging-api.onrender.com/api